{
    "manifest_version": 2,
    "name": "SURF_SOOTHE",
    "version": "1.0",
    "description": "blow off some steam while surfing the web",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }